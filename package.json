{
  "name": "aether-accordion",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://gitlab.com/desko27/aether-accordion.git",
  "author": "desko27 <desko27@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "lint": "sui-lint js && sui-lint sass",
    "precommit": "sui-precommit run",
    "test": "cross-env NODE_ENV=test mocha -r @s-ui/polyfills -r babel-register --file scripts/test-setup.js \"src/**/*.test.js\"",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --file scripts/test-setup.js \"src/**/*.test.js\""
  },
  "nyc": {
    "require": [
      "@s-ui/polyfills",
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "@s-ui/lint": "^2.20.0",
    "@s-ui/precommit": "^2.5.0",
    "@storybook/addon-notes": "^4.0.6",
    "@storybook/addons": "^4.0.6",
    "@storybook/core-events": "^4.0.6",
    "@storybook/html": "^4.0.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-sui": "^1.10.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "highlight.js": "^9.13.1",
    "husky": "0.13.4",
    "mocha": "^5.2.0",
    "node-sass": "^4.10.0",
    "normalize-css": "^2.3.1",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "raw-loader": "^0.5.1",
    "rxjs-compat": "^6.3.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "suitcss-base": "^5.0.0"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ],
    "parser": "babel-eslint"
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml",
  "babel": {
    "presets": [
      "sui"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "dependencies": {
    "@s-ui/polyfills": "^1.7.0"
  }
}
